        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="68,136,0,0" VerticalAlignment="Top" Width="292"/>
        <Label x:Name="label_Copy" Content="Seleccione Vehiculo" HorizontalAlignment="Left" Margin="68,105,0,0" VerticalAlignment="Top" Width="120"/>
