<Window x:Class="VehiculosOnlineSite.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VehiculosOnlineSite"
        mc:Ignorable="d"
        Title="Login" Height="248.875" Width="389.068">
    <Grid>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="122,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" MaxLength="70"/>
        <Button x:Name="btnEntrar" Content="Entrar" HorizontalAlignment="Left" Margin="141,137,0,0" VerticalAlignment="Top" Width="75" Click="btnEntrar_Click"/>
        <Label x:Name="label" Content="Ingrese usuario" HorizontalAlignment="Left" Margin="122,18,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Ingrese contraseña" HorizontalAlignment="Left" Margin="122,63,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="txtContrasena" HorizontalAlignment="Left" Margin="122,94,0,0" VerticalAlignment="Top" Width="120" Height="25" MaxLength="30"/>
        <Label x:Name="lblMensaje" Content="" HorizontalAlignment="Left" Margin="113,156,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
